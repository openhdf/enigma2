<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<skin>
	<windowstyle id="1" type="skinned">
		<color color="#000000" name="Background"/>
		<color color="#ffffff" name="LabelForeground"/>
		<color color="#000000" name="ListboxBackground"/>
		<color color="#ffffff" name="ListboxForeground"/>
		<color color="#000000" name="ListboxSelectedBackground"/>
		<color color="#ffffff" name="ListboxSelectedForeground"/>
		<color color="#000000" name="ListboxMarkedBackground"/>
		<color color="#ffffff" name="ListboxMarkedForeground"/>
		<color color="#000000" name="ListboxMarkedAndSelectedBackground"/>
		<color color="#ffffff" name="ListboxMarkedAndSelectedForeground"/>
		<color color="#ffffff" name="WindowTitleForeground"/>
		<color color="#000000" name="WindowTitleBackground"/>
	</windowstyle>
	<!-- mod by googgi for openhdf lx3  -->
	<fonts>
		<font filename="analog.ttf" name="VFD" scale="100" />
	</fonts>
<!-- LCD screen InfoBar -->
	<screen name="InfoBarSummary" position="0,0" size="256,64">
		<widget source="session.RecordState" render="FixedLabel" position="220,0" zPosition="3" size="32,12" text=" ">
			<convert type="ConfigEntryTest">config.usage.blinking_rec_symbol_during_recording,True,CheckSourceBoolean</convert>
			<convert type="ConditionalShowHide">Blink</convert>
		</widget>
		<widget source="session.RecordState" render="Pixmap" pixmap="/usr/share/enigma2/display/vfd_icons/rec.png" position="220,0" size="32,12" alphatest="blend">
			<convert type="ConditionalShowHide" />
		</widget>
		<widget source="session.CurrentService" render="LcdPicon" position="126,12" size="132,46" zPosition="1">
			<convert type="ServiceName">Reference</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="126,12" size="132,46" zPosition="0" font="VFD;18" halign="center" valign="center">
			<convert type="ServiceName">Name</convert>
		</widget>
		<widget source="global.CurrentTime" render="Label" position="0,22" size="92,34" font="VFD;34" halign="right" valign="center" transparent="1">
			<convert type="ClockToText">Format:%H:%M</convert>
		</widget>
		<widget source="global.CurrentTime" render="Label" position="94,24" zPosition="1" size="28,20" font="VFD;20" valign="center" transparent="1">
			<convert type="ClockToText">Format:%S</convert>
		</widget>
		<widget source="session.Event_Now" render="Progress" position="0,59" size="256,5" borderWidth="0">
			<convert type="EventTime">Progress</convert>
		</widget>
	</screen>
<!-- LCD screen InfoBarMoviePlayer -->
	<screen name="InfoBarMoviePlayerSummary" position="0,0" size="256,64">
		<widget source="session.RecordState" render="FixedLabel" position="220,0" zPosition="3" size="32,12" text=" ">
			<convert type="ConfigEntryTest">config.usage.blinking_rec_symbol_during_recording,True,CheckSourceBoolean</convert>
			<convert type="ConditionalShowHide">Blink</convert>
		</widget>
		<widget source="session.RecordState" render="Pixmap" pixmap="/usr/share/enigma2/display/vfd_icons/rec.png" position="220,0" size="32,12" alphatest="blend">
			<convert type="ConditionalShowHide" />
		</widget>
		<widget source="session.CurrentService" render="RollerCharLCDLong" position="0,13" size="256,24" font="VFD;24" noWrap="1" valign="top">
			<convert type="ServiceName">Name</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="0,38" size="85,18" font="VFD;18" halign="left" valign="top">
			<convert type="ServicePosition">Position</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="85,38" size="85,18" font="VFD;18" halign="center" valign="top">
			<convert type="ServicePosition">Length</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="170,38" size="85,18" font="VFD;18" halign="right" valign="top">
			<convert type="ServicePosition">Remaining</convert>
		</widget>
		<widget source="session.CurrentService" render="Progress" position="0,59" size="256,5" borderWidth="0">
			<convert type="ServicePosition">Position</convert>
		</widget>
	</screen>
<!-- LCD screen Channelselection -->
	<screen name="ChannelSelection_summary" position="0,0" size="256,64">
		<widget source="parent.ServiceEvent" render="RollerCharLCDLong" position="0,0" size="256,27" font="VFD;27" noWrap="1">
			<convert type="ServiceName">Name</convert>
		</widget>
		<widget source="parent.ServiceEvent" render="RollerCharLCDLong" position="0,28" size="256,27" font="VFD;25" noWrap="1" >
			<convert type="EventName">Name</convert>
		</widget>
		<widget source="parent.ServiceEvent" render="Progress" position="0,54" size="256,10" borderWidth="1">
			<convert type="EventTime">Progress</convert>
		</widget>
	</screen>
<!-- LCD screen (menus) -->
	<screen name="MenuSummary" position="0,0" size="256,64">
		<widget source="parent.title" render="RollerCharLCDLong" position="0,0" size="256,30" font="VFD;28" noWrap="1" />
		<widget source="parent.menu" render="RollerCharLCDLong" position="0,32" size="256,28" font="VFD;26" noWrap="1" >
			<convert type="StringListSelection" />
		</widget>
	</screen>
<!-- LCD screen (dvd player ) -->
	<screen name="DVDSummary" position="0,0" size="256,64">
		<widget source="session.RecordState" render="FixedLabel" position="220,0" zPosition="3" size="32,12" text=" ">
			<convert type="ConfigEntryTest">config.usage.blinking_rec_symbol_during_recording,True,CheckSourceBoolean</convert>
			<convert type="ConditionalShowHide">Blink</convert>
		</widget>
		<widget source="session.RecordState" render="Pixmap" pixmap="/usr/share/enigma2/display/vfd_icons/rec.png" position="220,0" size="32,12" alphatest="blend">
			<convert type="ConditionalShowHide" />
		</widget>
		<widget source="session.CurrentService" render="RollerCharLCDLong" position="0,13" size="256,24" font="VFD;24" noWrap="1" valign="top">
			<convert type="ServiceName">Name</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="0,38" size="85,18" font="VFD;18" halign="left" valign="top">
			<convert type="ServicePosition">Position</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="85,38" size="85,18" font="VFD;18" halign="center" valign="top">
			<convert type="ServicePosition">Length</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="170,38" size="85,18" font="VFD;18" halign="right" valign="top">
			<convert type="ServicePosition">Remaining</convert>
		</widget>
		<widget source="session.CurrentService" render="Progress" position="0,59" size="256,5" borderWidth="0">
			<convert type="ServicePosition">Position</convert>
		</widget>
	</screen>
<!-- LCD screen Standby -->
	<screen name="StandbySummary" position="0,0" size="256,64">
		<widget source="session.RecordState" render="FixedLabel" position="220,0" zPosition="3" size="32,12" text=" ">
			<convert type="ConfigEntryTest">config.usage.blinking_rec_symbol_during_recording,True,CheckSourceBoolean</convert>
			<convert type="ConditionalShowHide">Blink</convert>
		</widget>
		<widget source="session.RecordState" render="Pixmap" pixmap="/usr/share/enigma2/display/vfd_icons/rec.png" position="220,0" size="32,12" alphatest="blend">
			<convert type="ConditionalShowHide" />
		</widget>
		<widget source="global.CurrentTime" render="Label" position="54,16" size="113,32" font="VFD;40" halign="right" valign="center" transparent="1">
			<convert type="ClockToText">Format:%H:%M</convert>
		</widget>
		<widget source="global.CurrentTime" render="Label" position="169,14" zPosition="1" size="32,20" font="VFD;20" valign="center" transparent="1">
			<convert type="ClockToText">Format:%S</convert>
		</widget>
		<widget source="global.CurrentTime" render="Label" font="VFD;12" position="0, 50" size="128, 20" halign="center">
			<convert type="VtiTempFan">FanInfo</convert>
		</widget>
		
		<widget source="global.CurrentTime" render="Label" font="VFD;12" position="128, 50" size="128, 20" halign="center">
			<convert type="VtiTempFan">TempInfo</convert>
		</widget>
		<widget source="session.RecordState" render="FixedLabel" position="0,0" zPosition="1" size="256,64" text=" ">
			<convert type="ConfigEntryTest">config.usage.blinking_display_clock_during_recording,True,CheckSourceBoolean</convert>
			<convert type="ConditionalShowHide">Blink</convert>
		</widget>
	</screen>
<!-- LCD screen Service Scan -->
	<screen name="ServiceScanSummary" position="0,0" size="256,64">
		<widget name="Title" position="0,0" size="256,42" font="VFD;18" transparent="1" />
		<widget name="scan_progress" position="0,50" borderWidth="1" size="256,12" />
		<widget name="Service" position="0,22" size="256,26" font="VFD;16" transparent="1" />
	</screen>
<!-- LCD screen Mediaplayer -->
	<screen name="MediaPlayerLCDScreen" position="0,0" size="256,64" id="1">
		<widget name="text1" position="0,0" size="256,35" font="VFD;16"/>
		<widget name="text3" position="0,36" size="256,14" font="VFD;12"/>
		<widget name="text4" position="0,49" size="256,14" font="VFD;12"/>
	</screen>
<!-- LCD screen Simple Summary -->
	<screen name="SimpleSummary" position="0,0" size="256,64">
		<widget source="parent.Title" render="RollerCharLCDLong" position="0,0" size="256,30" font="VFD;28" noWrap="1" />
	</screen>
<!-- LCD screen PluginBrowser -->
	<screen name="PluginBrowserSummary" position="0,0" size="256,64">
		<widget source="entry" render="RollerCharLCDLong" position="0,0" size="256,30" font="VFD;28" noWrap="1" />
		<widget source="desc" render="RollerCharLCDLong" position="0,32" size="256,28" font="VFD;26" noWrap="1" />
	</screen>
<!-- LCD screen VTIPanel -->
	<screen name="VTIPanelSummary" position="0,0" size="256,64">
		<widget source="entry" render="RollerCharLCDLong" position="0,0" size="256,30" font="VFD;28" noWrap="1" />
		<widget source="desc" render="RollerCharLCDLong" position="0,32" size="256,28" font="VFD;26" noWrap="1" />
	</screen>
<!-- LCD screen DeviceManager2 -->
	<screen name="DeviceManager2Summary" position="0,0" size="256,64">
		<widget source="entry" render="RollerCharLCDLong" position="0,0" size="256,30" font="VFD;28" noWrap="1" />
		<widget source="desc" render="RollerCharLCDLong" position="0,32" size="256,28" font="VFD;26" noWrap="1" />
	</screen>
<!-- LCD screen Setup -->
	<screen name="SetupSummary" position="0,0" size="256,64">
		<widget source="SetupTitle" render="RollerCharLCDLong" position="6,0" size="256,24" font="VFD;22" noWrap="1" />
		<widget source="SetupEntry" render="RollerCharLCDLong" position="6,24" size="256,20" font="VFD;18" noWrap="1" />
		<widget source="SetupValue" render="RollerCharLCDLong" position="6,44" size="256,20" font="VFD;18" noWrap="1" />
	</screen>
<!-- LCD screen MessageBox -->
	<screen name="MessageBox_summary" position="0,0" size="256,64" id="1">
		<widget source="parent.Text" render="Label" position="0,0" size="256,45" font="VFD;14" halign="center" valign="center" />
		<widget source="parent.selectedChoice" render="Label" position="0,46" size="244,18" font="VFD;16" halign="center" valign="center" />
	</screen>
<!-- LCD screen ChoiceBox -->
	<screen name="ChoiceBox_summary" position="0,0" size="256,64" id="1">
		<widget source="parent.summary_list" render="Label" position="0,0" size="244,64" font="VFD;12" />
	</screen>
	<!-- VFD screen EMCMoviePlayerSummary -->
  <screen name="EMCMoviePlayerSummary" position="0,0" size="256,64">
		<widget source="session.CurrentService" render="Label" position="0,0" size="256,30" font="VFD;25" halign="left" noWrap="1">
		<convert type="ServiceName">Name</convert>
		</widget>
		<widget source="Service" render="Progress" position="0,32" size="256,10" borderWidth="2">
		<convert type="EMCServicePosition">Position</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="0,44" size="128,20" font="VFD;20" halign="left" valign="top">
			<convert type="ServicePosition">Length</convert>
		</widget>
		<widget source="session.CurrentService" render="Label" position="130,44" size="126,20" font="VFD;20" halign="right" valign="top">
			<convert type="ServicePosition">Remaining</convert>
		</widget>
	</screen>  
<!-- VFD screen EMCMoviePlayerSummary end -->
</skin> 









